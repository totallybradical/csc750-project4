package drools

import models.Bank;

rule "Hello World"
      dialect "mvel"
  when
      $b : Bank( $b.isBlacklisted() == false )
  then
      System.out.println( "Bank " + $b.getId() + " blacklist status: " + $b.isBlacklisted() );
      $b.setBlacklisted(true);
      System.out.println( "Bank " + $b.getId() + " blacklist status: " + $b.isBlacklisted() );
end